[
  {
    "type": "console",
    "level": "Info",
    "format": "{now:yyyy/MM/dd HH:mm:ss.fff},[{level,-5}],{thread:x},{category},{message}"
  },
  {
    "type": "debug",
    "level": "Debug",
    "format": "{now:yyyy/MM/dd HH:mm:ss.fff}:{message}"
  },
  {
    "level": "Info",
    "format": "{now:yyyy/MM/dd HH:mm:ss.fff},[{level,-5}],{thread:0000},{category},{message}",
    "filepath": "{assembly-name}.log",
    "rotation": {
      "count": 100,
      "size": "10M"
    }
  },
  {
    "level": "Error",
    "format": "{now:yyyy/MM/dd HH:mm:ss.fff},[{level,-5}],{thread:0000},{category},{message}",
    "filepath": "{assembly-name}-{level}_{datetime}.log",
    "rotation": {
      "count": 10,
      "size": "10M"
    }
  }
